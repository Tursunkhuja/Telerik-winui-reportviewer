<UserControl
    x:Class="General.WinUIClassLibraryApp.WinUIReportViewerUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:telerikReporting="using:Telerik.ReportViewer.WinUI"
    xmlns:telerikReportingCore="using:Telerik.Reporting"
    mc:Ignorable="d">

    <Grid>
        <telerikReporting:ReportViewer x:Name="reportViewer">
            <telerikReporting:ReportViewer.ReportSource>
                <telerikReportingCore:UriReportSource Uri="Reports\Barcodes Report.trdp" />
            </telerikReporting:ReportViewer.ReportSource>
        </telerikReporting:ReportViewer>
    </Grid>
</UserControl>
